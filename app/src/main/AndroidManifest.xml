<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.B17.sdssystem">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true">
        <activity
                android:name=".manager.ManagerActivity"
                android:label="@string/title_activity_manager"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".developer.DeveloperActivity">
        </activity>
        <activity android:name=".manager.assignment.AssignmentActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".authentication.AuthenticationActivity">
        </activity>
        <!--<activity android:name=".manager.ManagerActivity">-->
        <!--</activity>-->
    </application>

</manifest>